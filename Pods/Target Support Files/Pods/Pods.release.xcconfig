GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CocoaAsyncSocket" "${PODS_ROOT}/Headers/Public/CocoaLumberjack" "${PODS_ROOT}/Headers/Public/XMPPFramework" $(SDKROOT)/usr/include/libxml2 $(SDKROOT)/usr/include/libresolv
LIBRARY_SEARCH_PATHS = "$(PODS_ROOT)/XMPPFramework/Vendor/libidn" "$(PODS_ROOT)/XMPPFramework/Vendor/libidn"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CocoaAsyncSocket" -isystem "${PODS_ROOT}/Headers/Public/CocoaLumberjack" -isystem "${PODS_ROOT}/Headers/Public/XMPPFramework"
OTHER_LDFLAGS = -ObjC -l"Pods-CocoaAsyncSocket" -l"Pods-CocoaLumberjack" -l"Pods-XMPPFramework" -l"idn" -l"resolv" -l"xml2" -framework "CFNetwork" -framework "CoreData" -framework "CoreLocation" -framework "Security" -framework "SystemConfiguration"
PODS_ROOT = ${SRCROOT}/Pods